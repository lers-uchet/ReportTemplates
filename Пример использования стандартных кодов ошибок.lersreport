    &lt;Item1 Ref="15" Name="ReportTitle" FieldType="String" Expression="FormatString('Месячная ведомость учета тепловой энергии' + NewLine() + 'и теплоносителя за {0:MMMM} {1:yyyy}', ?DATE_START, ?DATE_START) + NewLine() + FormatString('(c {0:dd.MM.yyyy} по {1:dd.MM.yyyy})', ?DATE_START, ?DATE_END)" DataMember="ArchiveParent" /&gt;
