    &lt;Item7 Ref="21" Name="TempDiffLowErrorExists" FieldType="Boolean" Expression="Iif(?CheckConsumptionDataForIncidents, &amp;#xA;&amp;#x9;Not [NoFlowErrorExist] And Not IsNull([T_delta]) And [T_delta] &amp;lt; ?TempDifLow,&amp;#xA;&amp;#x9;[Стандартные коды ошибок].[TempDiffLowExist])" DataMember="Архив суточный" /&gt;
    &lt;Item8 Ref="22" Name="NoPowerErrorExist" FieldType="Boolean" Expression="Iif(?CheckConsumptionDataForIncidents, &amp;#xA;&amp;#x9;IsNull([NoPowerDuration], 0) &amp;gt; 0, &amp;#xA;&amp;#x9;[Стандартные коды ошибок].[NoPowerExist])" DataMember="Архив суточный" /&gt;
