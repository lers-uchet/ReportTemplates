aType == 64)&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;&amp;#x9;this.formatString = &amp;quot;{0:dd.MM.yyyy}&amp;quot;;&amp;#xD;&amp;#xA;&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;// Часовые данные&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;else if (dataType == 128)&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;&amp;#x9;this.formatString = &amp;quot;{0:dd.MM.yy HH:mm}&amp;quot;;&amp;#xD;&amp;#xA;&amp;#x9;}&amp;#xD;&amp;#xA;&amp;#xD;&amp;#xA;&amp;#x9;// Источник данных отчета&amp;#xD;&amp;#xA;&amp;#x9;DataSet dataSet = (DataSet)this.report.DataSource;&amp;#xD;&amp;#xA;&amp;#x9;if (!dataSet.Tables.Contains(&amp;quot;Параметры точки учета&amp;quot;))&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;return;&amp;#xD;&amp;#xA;&amp;#xD;&amp;#xA;&amp;#x9;DataTable table = dataSet.Tables[&amp;quot;Параметры точки учета&amp;quot;];&amp;#xD;&amp;#xA;&amp;#xD;&amp;#xA;&amp;#x9;// Теплопотери&amp;#xD;&amp;#xA;&amp;#x9;if(table.Columns.Contains(&amp;quot;HeatLoss&amp;quot;) &amp;amp;&amp;amp; table.Rows.Count &amp;gt; 0)&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;double.TryParse(Convert.ToString(table.Rows[0][&amp;quot;HeatLoss&amp;quot;]), out this.heatLoss);&amp;#xD;&amp;#xA;&amp;#xD;&amp;#xA;}&amp;#xD;&amp;#xA;&amp;#xD;&amp;#xA;// Меняем формат вывода дат отчетного периода в заголовке отчета в зависимости от типа данных&amp;#xD;&amp;#xA;private void labelReportPeriod_BeforePrint(object sender, System.Drawing.Printing.PrintEventArgs e) &amp;#xD;&amp;#xA;{&amp;#xD;&amp;#xA;&amp;#x9;if (this.report == null)&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;return;&amp;#xD;&amp;#xA;&amp;#xD;&amp;#xA;&amp;#x9;XRLabel label = (sender as XRLabel);&amp;#xD;&amp;#xA;&amp;#x9;if (label == null)&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;return;&amp;#xD;&amp;#xA;&amp;#xD;&amp;#xA;&amp;#x9;if (this.report.Parameters[&amp;quot;DATE_START&amp;quot;] != null &amp;amp;&amp;amp; label.Name == &amp;quot;labelDateFrom&amp;quot;)&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;label.Text = String.Format(this.formatString, (DateTime)this.report.Parameters[&amp;quot;DATE_START&amp;quot;].Value);&amp;#xD;&amp;#xA;&amp;#xD;&amp;#xA;&amp;#x9;if (this.report.Parameters[&amp;quot;DATE_END&amp;quot;] != null &amp;amp;&amp;amp; label.Name == &amp;quot;labelDateTo&amp;quot;)&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;label.Text = String.Format(this.formatString, (DateTime)this.report.Parameters[&amp;quot;DATE_END&amp;quot;].Value);&amp;#xD;&amp;#xA;}&amp;#xD;&amp;#xA;&amp;#xD;&amp;#xA;// Возвращает результат вычисляемого поля calculatedFieldDataDate&amp;#xD;&amp;#xA;// Вычисляемое поле calculatedFieldDataDate связано с ячейкой с именем DataDate&amp;#xD;&amp;#xA;private void calculatedFieldDataDate_GetValue(object sender, DevExpress.XtraReports.UI.GetValueEventArgs e) &amp;#xD;&amp;#xA;{&amp;#xD;&amp;#xA;&amp;#x9;if (String.IsNullOrEmpty(this.formatString))&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;return;&amp;#xD;&amp;#xA;&amp;#xD;&amp;#xA;&amp;#x9;DataRowView dataRowView = (e.Row as DataRowView);&amp;#xD;&amp;#xA;&amp;#xD;&amp;#xA;&amp;#x9;if (dataRowView == null)&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;return;&amp;#xD;&amp;#xA;&amp;#xD;&amp;#xA;&amp;#x9;if (!dataRowView.DataView.Table.Columns.Contains(&amp;quot;DataDate&amp;quot;))&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;return;&amp;#xD;&amp;#xA;&amp;#xD;&amp;#xA;&amp;#x9;// Задаем значение, которое является результатом вычисляемого поля&amp;#xD;&amp;#xA;&amp;#x9;if (e.Report.Parameters[&amp;quot;DATA_TYPE&amp;quot;] != null)&amp;#xD;&amp;#xA;&amp;#x9;{&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;// Тип данных&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;int dataType = (int)e.Report.Parameters[&amp;quot;DATA_TYPE&amp;quot;].Value;&amp;#xD;&amp;#xA;&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;// Величина сдвига в метках времени&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;// Если 0, то нет сдвига.&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;// Если больше 0, то сдвиг вперед&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;// Если меньше 0, то сдвиг назад&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;int shift = 1;&amp;#xD;&amp;#xA;&amp;#xD;&amp;#xA;&amp;#x9;&amp;#x9;// Месячные данные&amp;#xD;&amp;#x